{
  "name": "vishwac09/leet-code",
  "description": "Collection of Problems solved on LeetCode",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Vishwa Chikate",
      "email": "vishwa.chikate@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "php": "^8.0",
    "phpunit/phpunit": "^9",
    "vishwac09/php-algo-scaffold": "dev-master",
    "friendsofphp/php-cs-fixer": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "LeetCode\\Problems\\": "src/php"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LeetCode\\Tests\\": "tests/php"
    }
  },
  "scripts": {
    "tests": "./vendor/bin/phpunit --colors tests/php",
    "pretty:php": "php-cs-fixer fix --config=./.php-cs-fixer.php"
  }
}
